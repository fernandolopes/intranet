<%= form_for(:frequencia, :url => { :action => "create" }, :html => { :multipart => true }) do |f| %>

  <div class="field">
    <%= label 'upload_datafile', "Arquivo TXT:" %><br />
    <%= file_field 'upload', 'datafile' %>
  </div>
  <div class="actions">
    <%= f.submit "Enviar" %>
  </div>
<% end %>

