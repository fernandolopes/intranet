<table class="grid">
  <tr class="grid_title">
    <td colspan="9">Lista de Frequências</td>
  </tr>
  <tr>
    <td class="grid_label center">Data</td>
    <td class="grid_label center">Usuário</td>
    <td class="grid_label center">Setor</td>
    <td class="grid_label center">Profissão</td>
    <td class="grid_label center">Matricula</td>
    <td class="grid_label center">Hora 1</td>
    <td class="grid_label center">Hora 2</td>
    <td class="grid_label center">Total 1</td>
    <td class="grid_label center">Ações</td>
  </tr>

<% @frequencia_frequencias.each do |frequencia_frequencia| %>
  <tr>                                          <%#  raise frequencia_frequencia.inspect %>
    <% usuario = Usuario.find_by_matricula(frequencia_frequencia[:matricula]) %>
     <td class="grid_value center"><%= frequencia_frequencia[:data] %></td>
    <td class="grid_value center"><%= usuario.nome %></td>
    <td class="grid_value center"><%= usuario.setor.setor %></td>
    <td class="grid_value center"><%= usuario.profissao.profissao %></td>
    <td class="grid_value center"><%= frequencia_frequencia[:matricula] %></td>
    <td class="grid_value center"><%= frequencia_frequencia[:hora1] %></td>
    <td class="grid_value center"><%= frequencia_frequencia[:hora2] %></td>
    <td class="grid_value center"><%= frequencia_frequencia[:total1] %></td>
    <td class="grid_action center">
        <%= link_to image_tag("/images/icons/lupa.png",   :title => 'Mostrar'), frequencia_frequencia %>
        <%= link_to image_tag("/images/icons/delete.png", :title => 'Apagar'), frequencia_frequencia, :confirm => 'Você tem certeza?', :method => :delete %>
    </td>
  </tr>
<% end %>
  <tr>
    <td class="grid_footer" colspan="9"><%= raw "<strong>Total:</strong> #{@total}" %></td>
  </tr>
</table>